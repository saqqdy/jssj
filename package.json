{
    "name": "@jssj/monorepo",
    "description": "jssj",
    "version": "1.0.0-alpha.8",
    "scripts": {
        "eslint": "eslint --fix --ext .vue,.js",
        "prettier": "npx prettier --write ./",
        "lib": "sh scripts/lib.sh",
        "docs": "sh scripts/docs.sh --docs",
        "push": "tscjs scripts/push.ts",
        "build": "tscjs scripts/build.ts",
        "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
        "pub": "tscjs scripts/publish.ts",
        "unpub": "tscjs scripts/unpublish",
        "sync": "tscjs scripts/sync",
        "viewInfo": "tscjs scripts/viewInfo",
        "update": "tscjs scripts/update.ts",
        "dist": "pnpm run update && pnpm build",
        "clean": "rimraf dist types typings packages/*/dist"
    },
    "main": "index.js",
    "dependencies": {
        "js-cool": "^2.3.2"
    },
    "devDependencies": {
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@saqqdy/prettier-config": "^1.1.2",
        "@types/node": "^17.0.31",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "consola": "^2.15.3",
        "cross-env": "^7.0.3",
        "esbuild": "^0.14.38",
        "esbuild-register": "^3.3.2",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-sets": "^1.8.2",
        "eslint-plugin-tsdoc": "^0.2.16",
        "fast-glob": "^3.2.11",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.71.1",
        "rollup-plugin-dts": "^4.2.1",
        "rollup-plugin-esbuild": "^4.9.1",
        "rollup-plugin-replace-shebang": "^1.1.0",
        "rollup-plugin-visualizer": "^5.6.0",
        "tsnd": "^1.0.2",
        "typedoc": "^0.22.15",
        "typedoc-plugin-markdown": "^3.12.1",
        "typescript": "^4.6.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/jssj.git"
    },
    "keywords": [
        "jssj",
        "vue",
        "vue3.0"
    ],
    "author": "saqqdy.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/jssj/issues"
    },
    "homepage": "https://github.com/saqqdy/jssj#readme"
}
